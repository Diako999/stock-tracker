<div class="row">
  <div class="col-9">
    <table class="table table-hover table-dark mt-2">
      <thead>
        <tr>
          <th scope="col">Stock</th>
          <th scope="col">Name</th>
          <th scope="col">Price</th>
          <th scope="col">Actions</th>
        </tr>
      </thead>
      <tbody>
        <% @stocks.each do |stock| %>
          <tr>
            <th scope="row">1</th>
            <td><%= stock.stock %></td>
            <td><%= stock.name %></td>
            <td><%= stock.last_price %></td>
            <td>"action"</td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
