<div class="row">
  <div class="col-9">
    <table class="table table-hover table-bordered table-dark mt-2">
      <thead>
        <tr>
          <th scope="col">Stock</th>
          <th scope="col">Name</th>
          <th scope="col">Price</th>
          <th scope="col">Actions</th>
        </tr>
      </thead>
      <tbody>
        <% @stocks.each do |stock| %>
          <tr>
            <td><%= stock.ticker %></td>
            <td><%= stock.name %></td>
            <td><%= stock.last_price %></td>
            <td><%= link_to 'Utrack', user_stock_path(stock), method: :delete, data: {confirm: "sure??!!"}, class: "btn btn-sm  btn-danger" %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
